{
  "name": "electrified",
  "version": "1.0.0",
  "description": "Generates Electron apps from any web app ",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "generate": "babel-node src/index.js ",
    "jest": "jest",
    "test": "jest --watch",
    "lint": "eslint src --fix"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "devtron": "^1.4.0",
    "diff-match-patch": "^1.0.4",
    "electron-debug": "^2.1.0",
    "image-downloader": "^3.4.2",
    "nodegit": "^0.24.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.4.0",
    "electron": "^4.1.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^13.1.0",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globals.js"
    ]
  }
}
